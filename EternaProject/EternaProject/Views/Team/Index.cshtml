@model TeamViewModel
<main id="main">

    <!-- ======= Breadcrumbs ======= -->
    <section id="breadcrumbs" class="breadcrumbs">
        <div class="container">

            <ol>
                <li><a href="index.html">Home</a></li>
                <li>Team</li>
            </ol>
            <h2>Team</h2>

        </div>
    </section><!-- End Breadcrumbs -->
    <!-- ======= Team Section ======= -->
    <section id="team" class="team">
        <div class="container">

            <div class="row">
                @foreach (var item in Model.Teams)
                {

                    <div class="col-lg-4 col-md-6 d-flex align-items-stretch">
                        <div class="member">
                            <img src="~/assets/img/team/@item.ImageUrl" alt="">
                            <h4>@item.Fullname</h4>
                            <span>@item.Position</span>
                            <p>
                                @item.Description
                            </p>
                            <div class="social">
                                <a href=@item.TwitterUrl><i class="bi bi-twitter"></i></a>
                                <a href=@item.FacebookUrl><i class="bi bi-facebook"></i></a>
                                <a href=@item.InstagramUrl><i class="bi bi-instagram"></i></a>
                                <a href=@item.LinkedinUrl><i class="bi bi-linkedin"></i></a>
                            </div>
                        </div>
                    </div>
                }


            </div>

        </div>
    </section><!-- End Team Section -->

</main><!-- End #main -->
